{
  "env": {
    "commonjs": true,
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {},
  "rules": {
    "no-const-assign": "warn",
    "no-this-before-super": "warn",
    "no-undef": "warn",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "constructor-super": "warn",
    "valid-typeof": "warn"
  }
}